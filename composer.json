{
    "name": "staudenmeir/eloquent-json-relations",
    "description": "Laravel Eloquent relationships with JSON keys",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^12.0",
        "staudenmeir/eloquent-has-many-deep-contracts": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "larastan/larastan": "^3.0",
        "laravel/framework": "^12.0",
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench-core": "^10.0",
        "phpunit/phpunit": "^11.0",
        "staudenmeir/eloquent-has-many-deep": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\EloquentJsonRelations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Staudenmeir\\EloquentJsonRelations\\IdeHelperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
